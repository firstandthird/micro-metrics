    "hapi-mongodb": "6.2.0",
    "inert": "^4.0.2",
    "joi": "10.1.0",
    "lodash": "4.17.2",
    "logr": "1.19.0",
    "metrics-graphics": "^2.10.1",
    "rapptor": "1.3.2",
    "vision": "^4.1.0"
  },
  "devDependencies": {
    "clientkit": "^0.21.0",
